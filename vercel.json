{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "other",
  "functions": {
    "api/**/*.ts": {
      "runtime": "22.x"
    }
  },
  "env": {
    "NODE_ENV": "production",
    "NODE_VERSION": "18"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
