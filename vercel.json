{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "other",
  "nodeVersion": "20.x",
  "functions": {
    "api/**/*.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
